# Development environment overrides for Django (local Postgres via Docker)

# Django
DEBUG=True
RBS_SECRET_KEY=dev-secret-key

# Postgres (match docker-compose.dev.yml)
RBS_PG_DATABASE=robson_dev
RBS_PG_USER=robson
RBS_PG_PASSWORD=robson
RBS_PG_HOST=localhost
RBS_PG_PORT=5432

# Trading / Binance
BINANCE_USE_TESTNET=True
TRADING_ENABLED=False

# Binance API (testnet for development)
RBS_BINANCE_API_KEY_TEST=dev-test-api-key
RBS_BINANCE_SECRET_KEY_TEST=dev-test-secret-key

# Optional (prod keys not used in dev)
# RBS_BINANCE_API_KEY=
